

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model user {
  id            String @id @default(uuid())
  name          String
  email         String    @unique
  age           Int?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime @updatedAt

  @@map("user")
}
